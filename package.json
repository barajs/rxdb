{
  "name": "@barajs/rxdb",
  "version": "1.2.0",
  "description": "Bara RxDB integration module",
  "repository": {
    "type": "git",
    "url": "git@github.com:barajs/rxdb.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsnd --debounce 3000 --respawn server.ts",
    "compile": "yarn compile:tsc",
    "compile:tsc": "rm -rf dist && tsc -b",
    "compile:webpack": "webpack --mode=production",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "semantic-release": "semantic-release",
    "precompile": "yarn format"
  },
  "dependencies": {
    "@barajs/core": "3.0.0-alpha.430",
    "@barajs/formula": "3.0.0-alpha.470",
    "rxdb": "8.7.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@types/lru-cache": "7.10.9",
    "@types/redux": "3.6.0",
    "prettier": "2.7.1",
    "semantic-release": "17.4.7",
    "ts-loader": "9.3.0",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
